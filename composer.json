{
    "name": "izoniks/laravel-eloquent-mongodb-state-machines",
    "description": "State Machines for your Laravel Eloquent MongoDB models",
    "keywords": [
        "izoniks",
        "mongodb",
        "eloquent",
        "state",
        "machines",
        "state machines",
        "laravel",
        "laravel eloquent",
        "laravel mongodb",
        "mongodb state machines"
    ],
    "homepage": "https://github.com/AlexArtaud-Dev/laravel-eloquent-mongodb-state-machines",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alexandre Artaud",
            "email": "smlartaudalexandre@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "mongodb/laravel-mongodb": "^4.6",
        "javoscript/laravel-macroable-models": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Izoniks\\LaravelEloquentMongodbStateMachines\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Izoniks\\LaravelEloquentMongodbStateMachines\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Izoniks\\LaravelEloquentMongodbStateMachines\\LaravelEloquentStateMachinesServiceProvider"
            ]
        }
    }
}
